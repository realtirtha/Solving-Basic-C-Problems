            double faren, celsius;
            Console.WriteLine("Enter number in celsius = ");
            celsius = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("In celsius = " + celsius);
            faren = (1.8 * celsius) + 32;
            Console.WriteLine("In farenheit = "+ faren);
            celsius = (faren - 32) / 1.8;
            Console.WriteLine("In celsius = " + celsius);
